<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<listeners>
		<listener class-name="com.testing.utils.ReportingSD" />
	</listeners>

	<parameter name="browser" value="chrome"></parameter>


	<test thread-count="5" name="End-to-End WebTesting">
		<classes> 
			<class name="com.testing.testcases.TC01_Login" />  
			<class name="com.testing.testcases.TC02_SortProducts" />  
			<class name="com.testing.testcases.TC03_AddToCart" />  
			<class name="com.testing.testcases.TC04_Checkout" />   
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
